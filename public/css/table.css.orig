#the_table div.table-title {
  display: inline;
  margin: auto;
  max-width: 600px;
  padding:5px;
  width: 100%;
}

#the_table .table-title h3 {
   color: #fafafa;
   font-size: large;
   font-weight: 400;
   font-style:normal;
   font-family: 'Arvo', serif;
   text-shadow: -1px -1px 1px rgba(0, 0, 0, 0.1);
   text-transform:uppercase;
}


.table-fill {
  /*background: white;*/
